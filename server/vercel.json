{
    "version":2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "/"
        }
    ],
    "headers": [
      {
        "for": "/user/signup",  
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://open-library-psi.vercel.app"  
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, PUT, DELETE, OPTIONS"  
          }
         
        ]
      }]
  }